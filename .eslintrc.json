{
  "extends": "eslint:recommended",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "jsx-quotes": ["warn", "prefer-single"],
    "eol-last": ["warn", "always"],
    "indent": ["warn", 2],
    "key-spacing": ["warn"],
    "no-unused-vars": ["warn"],
    "no-undef": ["off"]
  }
}
